{
    "routes": [
        {
            "src": "/docs/(.*)",
            "dest": "/docs/$1"
        }
    ],
    "headers": [
        {
            "source": "/sitemap.xml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        }
    ]
}